<Defs>
  <!--==============================================NPC기본==============================================-->
  <PawnKindDef Name="Naki_ExplorerBase" Abstract="True">
    <race>Naki_Race</race>
    <defaultFactionType>Naki_HiddenFaction</defaultFactionType>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <invNutrition>3</invNutrition>
