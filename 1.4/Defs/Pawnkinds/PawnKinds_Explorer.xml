    <apparelIgnoreSeasons>false</apparelIgnoreSeasons>
    <initialWillRange>3~5</initialWillRange>
    <initialResistanceRange>6~10</initialResistanceRange>
  </PawnKindDef>
  <!--==============================================NPC세부==============================================-->

  <PawnKindDef ParentName="Naki_ExplorerBase">
    <defName>Naki_Citizen</defName>
    <label>Naki Citizen</label>
    <combatPower>20</combatPower>
    <itemQuality>Normal</itemQuality>
    <isFighter>false</isFighter>
    <apparelMoney>
      <min>1000</min>
      <max>1200</max>
    </apparelMoney>
    <apparelTags>
      <li>Naki_Citizen</li>
    </apparelTags>
    <apparelAllowHeadgearChance>0.2</apparelAllowHeadgearChance>
    <backstoryCategories>
      <li>Naki_Citizen</li>
    </backstoryCategories>
    <inventoryOptions>
      <skipChance>0</skipChance>
      <subOptionsChooseOne>
        <li>
          <choiceChance>25</choiceChance>
          <thingDef>DarkMatter</thingDef>
          <countRange>
            <min>1</min>
            <max>5</max>
          </countRange>
        </li>
      </subOptionsChooseOne>
    </inventoryOptions>
  </PawnKindDef>

